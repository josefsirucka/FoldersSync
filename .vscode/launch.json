    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/MyFolderSync/bin/Debug/net9.0/MyFolderSync.dll",
            "args": [
                "-l Information",
                "-p c:/temp/log.log",
                "-f olders c:\\sdftemtet",
                "-i 5"
            ],
            "cwd": "${workspaceFolder}/MyFolderSync"
        }

    ]
}